{
  "expo": {
    "name": "snaplistt",
    "slug": "test-snaplist",
    "version": "1.0.8",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "snaplist",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2A2661"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.daveclinton.snaplistt"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.daveclinton.snaplistt",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow snaplistt to access your camera",
          "microphonePermission": "Allow snaplistt to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "37e492b1-312c-4574-a7c2-d351d4b38f17"
      }
    },
    "owner": "daveclinton"
  }
}
